<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
        <f:entry title="${%Jacoco coverage counter type}" field="jacocoCoverageCounter">
          <select name="jacocoCoverageCounter">
            <f:option selected="${instance.jacocoCoverageCounter =='INSTRUCTION'}" value="INSTRUCTION">${%Instruction}</f:option>
            <f:option selected="${instance.jacocoCoverageCounter =='LINE'}" value="LINE">${%Line}</f:option>
            <f:option selected="${instance.jacocoCoverageCounter =='COMPLEXITY'}" value="COMPLEXITY">${%Complexity}</f:option>
            <f:option selected="${instance.jacocoCoverageCounter =='METHOD'}" value="METHOD">${%Method}</f:option>
            <f:option selected="${instance.jacocoCoverageCounter =='CLASS'}" value="CLASS">${%Class}</f:option>
          </select>
        </f:entry>
        <f:entry title="${%Publish result as}" field="publishResultAs">
          <select name="publishResultAs">
            <f:option selected="${instance.publishResultAs =='comment'}" value="comment">${%Comment}</f:option>
            <f:option selected="${instance.publishResultAs =='statusCheck'}" value="statusCheck">${%Status Check}</f:option>
          </select>
        </f:entry>
</j:jelly>
