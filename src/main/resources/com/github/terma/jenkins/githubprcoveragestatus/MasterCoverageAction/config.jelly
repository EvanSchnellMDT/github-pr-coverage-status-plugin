<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
        <f:entry title="${%Jacoco coverage counter type}" field="jacocoCoverageCounter">
          <select name="jacocoCounterType">
            <f:option selected="${instance.jacocoCounterType =='INSTRUCTION'}" value="INSTRUCTION">${%Instruction}</f:option>
            <f:option selected="${instance.jacocoCounterType =='LINE'}" value="LINE">${%Line}</f:option>
            <f:option selected="${instance.jacocoCounterType =='COMPLEXITY'}" value="COMPLEXITY">${%Complexity}</f:option>
            <f:option selected="${instance.jacocoCounterType =='METHOD'}" value="METHOD">${%Method}</f:option>
            <f:option selected="${instance.jacocoCounterType =='CLASS'}" value="CLASS">${%Class}</f:option>
          </select>
        </f:entry>
</j:jelly>
